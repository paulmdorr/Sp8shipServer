{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["plugin:@typescript-eslint/recommended", "prettier"],
  "ignorePatterns": ["node_modules", "dist", "src/generated"],
  "rules": {
    "prettier/prettier": "warn"
  }
}